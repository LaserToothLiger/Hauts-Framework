<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Obsidia Expansion - Tailborne Xenotype</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<!--filth rate genes affect track size-->
				<li Class="PatchOperationAdd">
					<xpath>/Defs/GeneDef[defName="OE_TailborneGrubby"]/statFactors</xpath>
					<value>
						<Hauts_TrackSize>1.2</Hauts_TrackSize>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/GeneDef[defName="OE_TailborneTidy"]/statFactors</xpath>
					<value>
						<Hauts_TrackSize>0.8</Hauts_TrackSize>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>